{
	"name": "ds_blobparameter",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_forblob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schema": {
				"type": "string"
			},
			"tablename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().schema,'_',dataset().tablename,'.csv')",
					"type": "Expression"
				},
				"container": "input"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}